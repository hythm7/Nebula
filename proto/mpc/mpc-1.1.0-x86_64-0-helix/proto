<proto>
  star mpc-1.1.0-x86_64-0-helix
  source https://ftp.gnu.org/gnu/mpc/mpc-1.1.0.tar.gz

<configure>

  ./configure                        \
    --prefix=/usr                    \
    --disable-static                 \
    --docdir=/usr/share/doc/mpc-1.1.0;

<compile>
  make -j [NPROC];

<install>
  make DESTDIR=[XYZ] install;

<desc>
  mpc.
