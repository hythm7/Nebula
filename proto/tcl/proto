<proto>
  star tcl-8.6.9-x86_64-0-helix
  source https://downloads.sourceforge.net/tcl/tcl8.6.9-src.tar.gz
  srcdir tcl8.6.9

<configure>
  cd unix;

  ./configure \
  --prefix=/tools;

<compile>
  cd unix;
  make -j [NPROC];

<install>
  cd unix;
  make DESTDIR=[XYZ] install install-private-headers;

<desc>
  Tool Command Language;
